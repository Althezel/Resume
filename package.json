{
    "name": "resume",
    "homepage": "https://Althezel.github.io/resume/",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.16.2",
        "@testing-library/react": "^12.1.3",
        "@testing-library/user-event": "^13.5.0",
        "gh-pages": "^3.2.3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-error-boundary": "^3.1.4",
        "react-router-dom": "^6.2.2",
        "react-scripts": "5.0.0",
        "sass": "^1.49.9",
        "typescript": "^4.6.2"
    },
    "scripts": {
        "start": "react-scripts start",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "build": "react-scripts build",
        "test": "jest",
        "test:watch": "jest --watch",
        "eject": "react-scripts eject"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/preset-react": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.21",
        "@types/react": "^17.0.39",
        "@types/react-dom": "^17.0.12",
        "@typescript-eslint/eslint-plugin": "^5.13.0",
        "@typescript-eslint/parser": "^5.13.0",
        "eslint": "^8.10.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.29.2",
        "eslint-plugin-react-hooks": "^4.3.0",
        "jest": "^27.5.1",
        "ts-jest": "^27.1.3"
    },
    "jest": {
        "verbose": true,
        "collectCoverage": true,
        "transform": {
            "^.+\\.(ts|tsx|js|jsx)$": "ts-jest"
        },
        "testEnvironment": "jsdom",
        "roots": [
            "src"
        ],
        "modulePaths": [
            "src"
        ],
        "testPathIgnorePatterns": [
            "<rootDir>/src/__tests__/__mocks__/"
        ],
        "moduleDirectories": [
            "node_modules"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__tests__/__mocks__/fileMock.js",
            "\\.(css|scss)$": "<rootDir>/src/__tests__/__mocks__/styleMock.js"
        }
    }
}
